#include <bits/stdc++.h>

using namespace std;
bool isPrime(int n) 
{ 
     
    if (n <= 1) 
        return false; 
    for (int i = 2; i < n/2; i++) 
        if (n % i == 0) 
            return false; 
    return true; 
} 
int main()
{
    int n,sum=0;
    cin>>n;
    vector<int>v(n);
    vector<int>a;
    for(int i=0;i<n;i++)
    {
        cin>>v[i];
    }
    for(int i=0;i<n;i++)
    {
        if(isPrime(v[i]))
          {
              a.push_back(v[i]);
          }
    }
    sort(a.begin(),a.end());
    for(int i=0;i<a.size();i++)
      sum+=a[i];
    cout<<sum;

    return 0;
}
